{
  "name": "carevo",
  "version": "1.0.0",
  "description": "Carevo is an open-source, voice-based chatbot integrated with WhatsApp, designed to deliver health education through audio messages in local languages.",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch --env-file .env.local src/index.ts",
    "start": "node --env-file .env.local dist/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SarkiMudboy/Carevo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SarkiMudboy/Carevo/issues"
  },
  "homepage": "https://github.com/SarkiMudboy/Carevo#readme",
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "imports": {
    "#*": "./src/*"
  }
}
